<% unless flash[:notices].nil? %>
  <% flash[:notices].each do |notice| %>
    <div class="alert alert-success" role="alert">
      <%= notice %>
    </div>
  <% end %>
<% end %>
<% unless flash[:errors].nil? %>
  <% flash[:errors].each do |error| %>
    <div class="alert alert-danger" role="alert">
      <%= error %>
    </div>
  <% end %>
<% end %>
